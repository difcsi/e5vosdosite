{
    "extends": [
        "react-app",
        "prettier",
        "plugin:storybook/recommended"
    ],
    "ignorePatterns": [
        "node_modules/"
    ],
    "plugins": [
        "unused-imports"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "rules": {
        "unused-imports/no-unused-imports": "error"
    }
}
